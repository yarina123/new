#main{
    padding-top:80px;
    width: 100%;
      background-color:#f1f1f6; 
      position: absolute;
     .logo{
            height:90px;
            width:100px;
            margin:0 auto;
            margin-bottom:20px;
           a{
           display: inline-block;
           width: 100%;
           height: 100%;
           background:url("../images/logo.jpg") no-repeat;
           background-position:-12px,-55px;
           }
       }
    form{
    background-color: #f0f0f5;

    label{
        font-size:18px;
        font-weight:400;
    }
   label,input{
        font-size: 16px;
   }
    }
    .register{
        border: 0.2px solid rgba(0,0,0,0.2);
        margin:30px 50px;
        text-align: center;
        height:40px;
        line-height:40px;
        a{
            color: green;
        }    
    }
    .other-login{
        p{
            line-height:30px;
            text-align: center;
        }
        a{
            display: block;
            img{
                width:50px;
            border-radius:30%;
            position: absolute;
            left: 50%;
            margin-left:-32px;
            }
        }
    }
    
}
