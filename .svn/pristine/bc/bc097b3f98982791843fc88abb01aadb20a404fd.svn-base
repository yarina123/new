@import "base.less";
#header{
    height:60px;
    line-height: 80px;
    color: white;
    font-size:24px;
    padding: 0 10px;
//  font-family: "KaiTi";
    text-align: center;

    #left{
        color: white;
        font-size:28px;
        font-weight:lighter;
        float: left;
        line-height:80px;
    }
    #right{
        span{
            font-size:20px;
            letter-spacing:2px;
        }
        float: right;
        a{
            margin-left: 10px;
            font-size:22px;
            color: white;
        }
    }
    
}









#main{
      padding-top: 50px;
      padding-bottom: 50px;
      height: 100%;
      background-color: #F3F3F3;
   ul{
       padding-top: 40px;
       li{
           padding:12px ;
          
           a{
               display: block;
               img{
                  width: 100%;
                   border-radius: 50px;
    }
           }
       }
   }
}

#footer{
    height:50px;
    background-color:rgba(255,255,255,0.5);
    border-top:1px solid rgba(0,0,0,0.1);
    .up{
        padding:0 5px;
        line-height: 50px;
        height: 100%;
        text-align: center;
    }
}
