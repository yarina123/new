* {
    margin: 0;
    padding: 0;
    box-sizing: border-box;
    /*touch-action: none;*/
  }
  li,
  a {
    list-style: none;
    text-decoration: 0;
  }
  i {
    font-style: normal;
  }
  html,
  body {
    height: 100%;
  }
  #header {
    background-color: #3b9c83;
    height: 70px;
    position: fixed;
    width: 100%;
    z-index: 999;
  }
  #header h4 {
    color: #f2f7fa;
    text-align: center;
    line-height: 90px;
    font-weight: normal;
    margin: 0;
    /* font-family: "SimHei"; */
    font-family: "微软雅黑";
    font-size: 18px;
  }
  #main {
    background-color: #f4f4f4;
  }
  #footer {
    background-color: #FFFDEF;
    border-top: 2px solid gainsboro;
    position: fixed;
    left: 0;
    width: 100%;
    height: 60px;
    line-height: 60px;
    bottom: 0;
    z-index: 999;
  }
  #footer li a {
    color: gray;
  }
  #footer li a.active {
    color: #188b64;
  }
  #footer li a.fa {
    font-size: 20px;
    text-align: center;
    display: block;
    padding-top: 8px;
  }
  #footer li a.fa span {
    display: block;
    margin-top: 5px;
    font-size: 16px;
  }
  